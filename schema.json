    },
    "blockchain": {
      "$id": "#root/blockchain",
      "title": "Blockchain",
      "type": "string",
      "default": "",
      "examples": ["Ethereum"],
      "pattern": "^.*$"
    },
    "stack": {
      "$id": "#root/stack",
      "title": "Stack",
      "type": "string",
      "default": "",
